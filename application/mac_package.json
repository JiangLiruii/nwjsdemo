{
  "name": "nwjs-demo",
  "main": "build/index.html",
  "build": {
    "nwVersion": "0.32.0",
    "packed": false,
    "nwFlavor": "sdk",
    "mute": false,
    "ffmpegIntegration": false,
    "files": [
      "build/**",
      "package.json"
